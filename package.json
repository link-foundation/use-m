{
  "name": "use-m",
  "version": "8.13.5",
  "description": "use-m: dynamically import any JavaScript module",
  "type": "module",
  "main": "use.cjs",
  "exports": {
    ".": {
      "import": "./use.mjs",
      "require": "./use.cjs"
    },
    "./use.cjs": "./use.cjs",
    "./use.mjs": "./use.mjs"
  },
  "bin": {
    "use": "./cli.mjs"
  },
  "scripts": {
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "test:examples": "./examples/test-examples.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/link-foundation/use-m.git"
  },
  "bugs": {
    "url": "https://github.com/link-foundation/use-m/issues"
  },
  "keywords": [
    "use",
    "import",
    "require",
    "npm",
    "backend",
    "frontend",
    "auto load",
    "auto install",
    "cache",
    "global require",
    "global import",
    "dynamic require",
    "dynamic import",
    "package loader",
    "module loader",
    "dependency management",
    "zx",
    "execa",
    "esm.sh",
    "esm",
    "unpkg.com",
    "unpkg"
  ],
  "author": "link-foundation",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "babel-jest": "^29.7.0",
    "command-stream": "^0.0.5",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "puppeteer": "^24.16.2"
  }
}
