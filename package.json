{
  "name": "use-m",
  "version": "8.10.26",
  "description": "use-m: dynamically import any JavaScript module",
  "type": "module",
  "main": "use.cjs",
  "exports": {
    ".": {
      "import": "./use.mjs",
      "require": "./use.cjs"
    },
    "./use.cjs": "./use.cjs",
    "./use.mjs": "./use.mjs",
    "./package.json": "./package.json"
  },
  "bin": {
    "use": "./cli.mjs"
  },
  "scripts": {
    "test": "yarn run test:node && yarn run test:bun",
    "test:node": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --config ./jest.config.js",
    "test:bun": "bun test",
    "test:bun:jest": "NODE_OPTIONS=\"--experimental-vm-modules\" bun x jest --config ./jest.config.js --runInBand",
    "test:examples": "./examples/test-examples.mjs",
    "publish:major": "yarn publish --major --access public && git push && git push --tags",
    "publish:minor": "yarn publish --minor --access public && git push && git push --tags",
    "publish:patch": "yarn publish --patch --access public && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/link-foundation/use-m.git"
  },
  "bugs": {
    "url": "https://github.com/link-foundation/use-m/issues"
  },
  "keywords": [
    "use",
    "import",
    "require",
    "npm",
    "backend",
    "frontend",
    "auto load",
    "auto install",
    "cache",
    "global require",
    "global import",
    "dynamic require",
    "dynamic import",
    "package loader",
    "module loader",
    "dependency management",
    "zx",
    "execa",
    "esm.sh",
    "esm",
    "unpkg.com",
    "unpkg"
  ],
  "author": "link-foundation",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0"
  }
}
